{
  "name": "xfc",
  "version": "1.6.1",
  "description": "A Cross Frame Container that handles securely embedding web content into a 3rd party domain",
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "babel": "babel src --out-dir lib",
    "build": "yarn run test && yarn run clean && yarn run lint && yarn run babel && yarn run webpack",
    "clean": "rm -rf dist lib doc",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "docs": "jsdoc  --package package.json -d doc ./src/**/*.js",
    "lint": "eslint 'src/**/*.js' --fix",
    "test": "mocha --recursive --compilers js:babel-core/register test/*.js",
    "webpack": "webpack -p --progress --colors --display-error-details"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "jsonrpc-dispatch": "^2.3.0",
    "mutation-observer": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jsdoc": "^3.5.5",
    "jsdom": "13.0.0",
    "jsdom-global": "3.0.2",
    "matchdep": "^2.0.0",
    "mocha": "^5.2.0",
    "sinon": "^7.1.1",
    "sinon-test": "^2.4.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cerner/xfc.git"
  }
}
